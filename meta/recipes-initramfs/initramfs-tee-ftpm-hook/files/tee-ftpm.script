PREREQ="tee-supplicant"
prereqs()
{
	echo "$PREREQ"
}
case $1 in
prereqs)
	prereqs
	exit 0
	;;
esac

. /scripts/functions

FTPM_DEV=/dev/tpmrm0
if ! test -c "${FTPM_DEV}"; then
    panic "Can't discover the fTPM device ${FTPM_DEV}!"
fi
